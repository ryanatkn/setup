{
  "name": "@ryanatkn/setup",
  "description": "setup instructions and resources for my dev environment",
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://github.com/ryanatkn/setup",
  "engines": {
    "node": ">=18.6"
  },
  "devDependencies": {
    "@feltcoop/eslint-config": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@feltcoop"
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
